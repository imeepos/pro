        "@nestjs/core": "^10.0.0",
        "@nestjs/typeorm": "^10.0.0",
        "@pro/core": "workspace:*",
        "@pro/entities": "workspace:*",
        "@pro/logger": "workspace:*",
        "@pro/mongodb": "workspace:*",
        "@pro/rabbitmq": "workspace:*",
        "@pro/redis": "workspace:*",
        "@pro/types": "workspace:*",
        "@pro/weibo": "workspace:*",
        "@pro/weibo-persistence": "workspace:*",
        "@pro/workflow-core": "workspace:*",
        "cheerio": "^1.1.2",
        "ioredis": "^5.3.2",
