{
  "name": "@pro/api",
  "version": "1.0.0",
  "description": "Pro API Service",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "pm2:dev": "pm2 start --name pro-api pnpm -- run start:dev",
    "pm2:prod": "pm2 start --name pro-api pnpm -- run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit --project tsconfig.build.json",
    "test": "jest --config ./test/jest-integration.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --config ./test/jest-integration.json",
    "test:all": "jest && npm run test:integration"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "11.0.13",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.13",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/microservices": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.13",
    "@nestjs/platform-socket.io": "^11.0.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "11.0.5",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/websockets": "^11.0.0",
    "@pro/entities": "workspace:*",
    "@pro/logger": "workspace:*",
    "@pro/minio": "workspace:*",
    "@pro/mongodb": "workspace:*",
    "@pro/rabbitmq": "workspace:*",
    "@pro/redis": "workspace:*",
    "@pro/sdk": "workspace:*",
    "@pro/types": "workspace:*",
    "@pro/utils": "workspace:*",
    "axios": "1.7.9",
    "bcryptjs": "2.4.3",
    "cache-manager": "^7.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dataloader": "^2.2.3",
    "graphql": "^16.11.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^5.8.1",
    "mongoose": "^8.0.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "8.13.1",
    "playwright": "^1.56.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@pro/eslint-config": "workspace:*",
    "@pro/prettier-config": "workspace:*",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "graphql-ws": "^5.16.0",
    "jest": "30.0.0-alpha.5",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.8.0",
    "ws": "^8.18.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
