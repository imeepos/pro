  "compilerOptions": {
    "deleteOutDir": true,
    "tsConfigPath": "tsconfig.build.json",
    "assets": [
      {
        "include": "assets/dict/**/*",
        "outDir": "dist/assets/dict"
      }
    ]
  }
}
