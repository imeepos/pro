# Aggregation Settings
HOURLY_WINDOW_SIZE=60
DAILY_ROLLUP_HOUR=3
WINDOW_UPDATE_INTERVAL=300000

# Cache TTL (seconds)
CACHE_TTL_HOURLY=86400
CACHE_TTL_DAILY=604800
CACHE_TTL_REALTIME=300
