# 环境配置
NODE_ENV=development
PORT=3004
LOG_LEVEL=debug

# PostgreSQL 配置
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres123
POSTGRES_DB=pro

# 或使用 DATABASE_URL (优先级更高)
# DATABASE_URL=postgresql://postgres:postgres123@localhost:5432/pro

# MongoDB 配置
MONGODB_URL=mongodb://localhost:27017/pro

# RabbitMQ 配置
RABBITMQ_URL=amqp://localhost:5672

# Cleaner 设置
BATCH_SIZE=50
CONCURRENT_TASKS=5
