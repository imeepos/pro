# 应用配置
NODE_ENV=development
PORT=3001

# API 服务地址
API_URL=http://api:3000

# RabbitMQ 配置
RABBITMQ_URL=amqp://rabbitmq:rabbitmq123@rabbitmq:5672
RABBITMQ_QUEUE=crawler-queue

# MongoDB 配置
MONGODB_URL=mongodb://mongo:mongo123@mongo:27017/pro?authSource=admin

